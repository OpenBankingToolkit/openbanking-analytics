{
  "name": "forgerock-openbanking-ui",
  "project_version": "MADNESS-SNAPSHOT",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test --passWithNoTests",
    "test.watch": "ng test --watch",
    "test.ci": "ng test --ci --bail --passWithNoTests",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix",
    "lint": "ng lint",
    "build:licenses": "./scripts/getDependenciesAndLicenses.js",
    "customize": "node scripts/applyCustomizations.js",
    "// PROJECT ANALYTICS": "------ANALYTICS-------",
    "serve.analytics": "ng serve analytics",
    "serve.analytics.server": "cd ./projects/analytics/server && npm run serve",
    "report.analytics": "npm run build.analytics.forgerock && webpack-bundle-analyzer dist/forgerock/stats.json",
    "build.analytics.forgerock": "ng build --project=analytics --prod --configuration=forgerock --output-path dist/forgerock --statsJson --extra-webpack-config webpack.extra.js",
    "build.analytics.all": "./scripts/build_all.sh -a analytics -c hackathon -c lloyds -c hl -c vanquis -c jpmorgan -c citibank",
    "=================CLUSTER UI-INTEG=================": "",
    "serve.ui-integ.analytics": "run-p serve.ui-integ.analytics.client serve.analytics.server",
    "serve.ui-integ.analytics.hl": "run-p serve.ui-integ.analytics.hl.client serve.analytics.server",
    "serve.ui-integ.analytics.hl.client": "ng serve analytics --host dev.analytics.ui-integ.forgerock.financial --browserTarget analytics:build:ui-integ-hl --sslCert ssl/all.ui-integ.forgerock.financial.crt --sslKey ssl/k-all.ui-integ.forgerock.financial.key",
    "serve.ui-integ.analytics.bendigobank.client": "ng serve analytics --host dev.analytics.ui-integ.forgerock.financial --browserTarget analytics:build:ui-integ-bendigobank --sslCert ssl/all.ui-integ.forgerock.financial.crt --sslKey ssl/k-all.ui-integ.forgerock.financial.key",
    "serve.ui-integ.analytics.citibank.client": "ng serve analytics --host dev.analytics.ui-integ.forgerock.financial --browserTarget analytics:build:ui-integ-citibank --sslCert ssl/all.ui-integ.forgerock.financial.crt --sslKey ssl/k-all.ui-integ.forgerock.financial.key",
    "serve.ui-integ.analytics.jpmorgan.client": "ng serve analytics --host dev.analytics.ui-integ.forgerock.financial --browserTarget analytics:build:ui-integ-jpmorgan --sslCert ssl/all.ui-integ.forgerock.financial.crt --sslKey ssl/k-all.ui-integ.forgerock.financial.key",
    "serve.ui-integ.analytics.client": "ng serve analytics --host dev.analytics.ui-integ.forgerock.financial --browserTarget analytics:build:ui-integ --sslCert ssl/all.ui-integ.forgerock.financial.crt --sslKey ssl/k-all.ui-integ.forgerock.financial.key",
    "================= LOCALHOST =================": "",
    "serve.localhost.analytics.client": "ng serve analytics --host 0.0.0.0 --browserTarget analytics:build:localhost --ssl false",
    "serve.localhost.analytics": "run-p serve.localhost.analytics.client serve.analytics.server",
    "start": "npm run serve.localhost.analytics"
  },
  "private": true,
